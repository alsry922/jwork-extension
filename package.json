{
  "name": "jwork-extension",
  "displayName": "jwork-extension",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jwork-extension.generate.controller",
        "title": "Generate Controller"
      },
      {
        "command": "jwork-extension.openChat",
        "title": "Open Chat Panel"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "jwork-chat-panel",
          "title": "JWork Chat",
          "icon": "$(comment-discussion)"
        }
      ]
    },
    "views": {
      "jwork-chat-panel": [
        {
          "id": "jwork-chat-panel",
          "name": "Chat Panel",
          "type": "webview"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.93.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}
